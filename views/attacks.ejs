                        <h2>Live Attacks</h2>
                        <% if(member.isADM || member.isHC || member.isBC) { %>
                            <div class="btn-toolbar" role="toolbar" aria-label="Button Toolbar">
                                <div class="btn-group flex-fill" role="group" aria-label="">
                                    <a class="btn btn-success" href="/att/new"><i class="fas fa-plus" aria-hidden="true"></i></a>
                                </div>
                            </div>
                        <% } %>
                        <table class="table table-sm table-striped w-100 table-align-middle">
                            <thead>
                            <tr>
                                <th scope="col"></th>
                                <th scope="col" class="fit-content">Number</th>
                                <th scope="col" class="fit-content">Land Tick</th>
                                <th scope="col">Title</th>
                                <th scope="col">Comment</th>
                                <th scope="col"></th>
                            </tr>
                            </thead>
                            <tbody>
                            <% for(var actAtt = 0; actAtt < attacks.length; actAtt++) { %>
                                <% if(attacks[actAtt].landtick + (attacks[actAtt].waves - 1) + after_land_ticks > tick.tick && (member.isCMDR || attacks[actAtt].releasetick <= tick.tick)) { %>
                                    <tr>
                                        <td class="fit-content no-wrap">
                                            <% if(member.isADM || member.isHC || member.isBC) { %>
                                                <a class="btn btn-sm btn-warning" href="/att/edit/<%= attacks[actAtt].hash %>"><i class="fas fa-pen" aria-hidden="true"></i></a>
                                                <form method="POST" action="/att/delete/<%= attacks[actAtt].hash %>" class="d-inline-block">
                                                    <button class="btn btn-sm btn-danger" type="submit" name="delete" value="<%= attacks[actAtt].hash %>"><i class="fas fa-times" aria-hidden="true"></i></button>
                                                </form>
                                            <% } %>
                                        </td>
                                        <td class="fit-content"><%= attacks[actAtt].number %></td>
                                        <td class="fit-content"><%= attacks[actAtt].landtick %></td>
                                        <td><%= attacks[actAtt].title %></td>
                                        <td><%= attacks[actAtt].comment %></td>
                                        <td class="fit-content no-wrap"><% if(attacks[actAtt].releasetick > tick.tick && member.isCMDR) { %>Released: pt<%= attacks[actAtt].releasetick %><% } %></td>
                                        <td class="fit-content no-wrap">
                                            <a class="btn btn-sm btn-primary" href="/att/<%= attacks[actAtt].hash %>"><i class="fas fa-rocket" aria-hidden="true"></i></a>
                                        </td>
                                    </tr>
                                <% } %>
                            <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="col-xl-2 col-lg-1 col-md-0"></div>
            </div>
            <div class="row">
                <div class="col-xl-2 col-lg-1 col-md-0"></div>
                <div class="col-xl-8 col-lg-10 col-md-12">
                    <div class="content">
                        <h2>Attack history</h2>
                        <table class="table table-sm table-striped w-100">
                            <thead>
                            <tr>
                                <th scope="col"></th>
                                <th scope="col" class="fit-content">Number</th>
                                <th scope="col" class="fit-content">Land Tick</th>
                                <th scope="col">Title</th>
                                <th scope="col">Comment</th>
                                <th scope="col"></th>
                            </tr>
                            </thead>
                            <tbody>
                            <% for(var histAtt = 0; histAtt < attacks.length; histAtt++) { %>
                                <% if(attacks[histAtt].landtick + (attacks[histAtt].waves - 1) + after_land_ticks <= tick.tick) { %>
                                    <tr>
                                        <td class="fit-content no-wrap">
                                            <% if(member.isADM || member.isHC || member.isBC) { %>
                                                <a class="btn btn-sm btn-warning" href="/att/edit/<%= attacks[histAtt].hash %>"><i class="fas fa-pen" aria-hidden="true"></i></a>
                                                <form method="POST" action="/att/delete/<%= attacks[histAtt].hash %>" class="d-inline-block">
                                                    <button class="btn btn-sm btn-danger" type="submit" name="delete" value="<%= attacks[histAtt].hash %>"><i class="fas fa-times" aria-hidden="true"></i></button>
                                                </form>
                                            <% } %>
                                        </td>
                                        <td class="fit-content"><%= attacks[histAtt].number %></td>
                                        <td class="fit-content"><%= attacks[histAtt].landtick %></td>
                                        <td><%= attacks[histAtt].title %></td>
                                        <td><%= attacks[histAtt].comment %></td>
                                        <td class="fit-content no-wrap">
                                            <a class="btn btn-sm btn-primary" href="/att/<%= attacks[histAtt].hash %>"><i class="fas fa-rocket" aria-hidden="true"></i></a>
                                        </td>
                                    </tr>
                                <% } %>
                            <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="col-xl-2 col-lg-1 col-md-0"></div>
            </div>
        </div>
    <% } %>
</main>
<footer>
    <%- include("templates/footer"); %>
</footer>
</body>
</html>
