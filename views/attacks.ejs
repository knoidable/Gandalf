                        <script>new ClipboardJS('#clipboard-button');</script>
                    </div>
                </div>
                <div class="col-xl-2 col-lg-1 col-md-0"></div>
            </div>
            <% } %>
        </div>
    <% } else if(page === "new" || page === "edit") { %>
        <div class="container-fluid">
