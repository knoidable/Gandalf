                                                <% } %>
                                            </select>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <div class="input-group input-group-sm">
                                            <div class="input-group-prepend">
                                                <span id="label-waves" class="input-group-waves">Waves</span>
                                            </div>
                                            <input type="text" name="waves" id="waves" class="form-control" aria-describedby="label-waves" value="<% if(page === "edit") { %><%= attack.waves %><% } else { %><%= waves %><% } %>"/>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <div class="input-group input-group-sm">
                                            <div class="input-group-prepend">
