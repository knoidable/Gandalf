                                                <% if(typeof (targets[i].scans.p) != "undefined" && targets[i].scans.p != null) { %>
                                                    <a class="btn btn-scan <% if(targets[i].scans.p.tick > tick.tick - expiries.P.bestbefore) { %>btn-success<% } else if(targets[i].scans.p.tick > tick.tick - expiries.P.expiry) { %>btn-warning<% } else { %>btn-danger<% } %>" href="<%= scanurl %>?scan_id=<%= targets[i].scans.p.scan_id %>" target="_blank">P</a>
