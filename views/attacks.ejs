                                                <% } %>
                                            </select>
                                        </div>
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </form>
                    </div>
                </div>
                <div class="col-xl-2 col-lg-1 col-md-0"></div>
            </div>
            <% if(page === "edit") { %>
