                        <h2>Coordinates:</h2>
                        <div class="input-group input-group-sm">
                            <input type="text" id="coords-list" class="form-control" value="<% targets.forEach(function(trg) { %><%= trg.x %>:<%= trg.y %>:<%= trg.z %> <% }); %>" readonly="readonly" />
