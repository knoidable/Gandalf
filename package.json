{
  "name": "middle_earth",
  "description": "Planetarion Tools",
  "keywords": [
    "Planetarion"
  ],
  "private": true,
  "imports": {
    "#Galadriel": "./Galadriel/galadriel.js",
    "#Mordor": "./Mordor/mordor.js",
    "#Bilbo": "./Bilbo/bilbo.js",
    "#Frodo": "./Frodo/frodo.js",
    "#Gandalf": "./Gandalf/gandalf.js",
    "#Sauron": "./Sauron/sauron.js"
  },
  "scripts": {
    "install:bilbo": "cd Bilbo && npm install",
    "install:frodo": "cd Frodo && npm install",
    "install:galadriel": "cd Galadriel && npm install",
    "install:gandalf": "cd Gandalf && npm install",
    "install:mordor": "cd Mordor && npm install",
    "install:sauron": "cd Sauron && npm install",
    "install": "npm run install:mordor && npm run install:galadriel && npm run install:bilbo && npm run install:frodo && npm run install:gandalf && npm run install:sauron",
    "start": "pm2 start MiddleEarth.js",
    "restart": "pm2 restart MiddleEarth.js",
    "stop": "pm2 stop MiddleEarth.js",
    "delete": "pm2 delete MiddleEarth.js",
    "list": "pm2 list",
    "watch": "pm2 monit"
  },
  "author": "Craig Roberts <craig.roberts@crdevel.com>",
  "contributors": [
    "Braden Edmunds <braden.edmunds@gmail.com>",
    "Alex High <freshlondon.biz@gmail.com>"
  ],
  "license": "GPL-3.0",
  "repository": "github:moldypenguins/Gandalf",
  "bugs": "https://github.com/moldypenguins/Gandalf/issues",
  "homepage": "https://github.com/moldypenguins/Gandalf",
  "engines": {
    "node": "^16.18.0",
    "npm": "^8.19.2",
    "mongodb": "^6.0.2"
  },
  "packageManager": "npm@8.19.2"
}
